{
  "schemaVersion": 1,
  "id": "mod-updater",
  "version": "${version}",
  "name": "Mod Updater",
  "description": "Update mods without ever opening a file system. Great for servers!",
  "authors": [
    "SpaceClouds42"
  ],
  "contact": {
    "homepage": "",
    "sources": "https://github.com/The-Tiny-Taters/Mod-Updater",
    "issues": "https://github.com/The-Tiny-Taters/Mod-Updater/issues"
  },
  "license": "LGPL",
  "icon": "assets/mod_updater/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "the_tiny_taters.mod_updater.Common::INSTANCE"
      }
    ]
  },
  "mixins": [
    "mod_updater.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.7.1",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.5.0+kotlin.1.4.31",
    "minecraft": "^1.16.5"
  }
}